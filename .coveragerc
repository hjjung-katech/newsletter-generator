[run]
source = newsletter
omit = 
    newsletter/__main__.py
    newsletter/cli.py:170-300
    newsletter/deliver.py:50-120

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    
[html]
directory = coverage_html_report 